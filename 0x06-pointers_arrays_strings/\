#include "main.h"
#include <string.h>
/**
 * _strncpy - copies string from source to destination
 *
 * @dest: desination string
 * @src: source string
 * @n: no of bytes for string
 * 
 * Return: concatenated dest string
 */
char *_strncpy(char *dest, char *src, int n)
{
	strncpy(dest, src, n);
	return (dest);
}
